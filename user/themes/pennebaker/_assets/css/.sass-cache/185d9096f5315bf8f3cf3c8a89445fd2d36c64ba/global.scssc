/* ================================================================================= STRUCTURE == */

#wrapper { 

    width: columns-(16);
    margin: 0 auto;
    -webkit-box-shadow: 0 0 1em #ccc;
    -moz-box-shadow: 0 0 1em #ccc;
    box-shadow: 0 0 1em #ccc;

    #masthead { 
        background: #fff; 
        #branding {
            padding: 20px;
            
            h1 { 
                font-size: 2em;
                margin: 0; 
                a { text-decoration: none; } } 
            h2 { 
                font-family: 'Open Sans', sans-serif, Helvetica, Arial;
                margin: 0; } }
        #nav { } }

    #content { 
    
        background: #fff;
        
        @include clearfix;
        width: columns-(16);
        
        #primaryContent { 

            background: #fff;
            @include grid-(12);
            
            #post-nav { } 
            section { 
                margin: 0 20px 20px 20px;
                @include clearfix;
                
                h1 { }
                .cal { } 
                .entry { }
                .entryMeta { } } /* post */ 
            section#comments { 
                h1 { }
                section#commentlist { 
                    h1 { } 
                    ul { } }
                #commentform {
                    form { } } } }

        #secondaryContent { 
        
            background: #fff;
            @include grid-(4);
            
            section { 
                margin: 0 10px 10px; } } }

#footer { 
    background: #666;
    padding: 5px 20px;
    color: #f5f5f5;
    section { }
    p { margin: 0; } } }
  

/* ================================================================================= HEADINGS == */

h1 {
    font-size: 1em;
    font-family: 'Sansita One';
    color: #545454; }

h2 {
    font-family: 'Sansita One';
    color: #545454; }

h3 { 
    font-family: 'Sansita One'; }

#branding {
    h1 { } }

#primaryContent {
    h1 { 
        font-size: 2em;
        @include reset-lineheight;
        a { 
            text-decoration: none; } }
    
    h2 { margin: 0; } }

#secondaryContent {
    h1 { }
    h2 { } }

#site, #rss, #aside, #commentheading {}


/* ================================================================================= LINKS == */

a {
  &:link {
    color: #222; }
  &:visited {
    color: #222;
    text-decoration: none; }
  &:hover {
    text-decoration: none;
    color: #A37941; }
  &:active {
    color: #222; } }

ul#nav li a {
  &:link, &:visited {
    display: block;
    width: 200px;
    padding-bottom: 5px;
    color: #666;
    text-decoration: none;
    text-indent: 20px; }
  &:hover {
    color: #000; } }

#branding a {
  border: none; }

ul#tidbits a {
  text-decoration: none; }
  

/* ================================================================================= MASTHEAD == */

#masthead {
    border-top: 3px #d71c4b solid; }


/* ================================================================================= MAIN NAV == */

#nav { 
    background: #222;
    border-top: 3px #d71c4b solid;
    border-bottom: 5px #ccc solid;
    ul { 
        @include clearfix;
        li { 
            float: left;
            a { 
                text-decoration: none;
                display: block;
                color: #fff;
                padding: 10px 20px; } } } }


/* ================================================================================= PRIMARY CONTENT == */

#post-nav {
    @include clearfix;
    margin: 10px 10px 0 10px; }

.cal {
    @include reset-lineheight;
    padding: 5px;
    width: 70px;
    float: left;
    margin: 0  12px 7px 0;
    text-align: center;
    border: 1px solid #444;

    .calyear {
        background: #444;
        color: #fff;
        padding: 3px 14px;
        border: 1px #444 solid; }
    
    .calday {
        margin: 0;
        display: block;
        background: #fff;
        border-left: 1px #444 solid;
        border-right: 1px #444 solid;
        font-size: 2em;
        padding: 5px 0;
        text-align: center;
        color: #d71c4b; }
    
    .calmonth {
        margin: 0;
        display: block;
        background: #e0e0e0;
        border: 1px #444 solid;
        padding: 5px 0;
        border-top: 1px solid #444;
        color: #d71c4b; } }

.entry { }

.draft {
  background: #FFF6BF;
  color: #514721;
  border-color: #FFD324;
  padding: 5px; }

.entry {
    min-height: 7em;
    ul {
        list-style-type: square;
        padding-left: 80px; }
    li {
        padding-bottom: 4px; } }

.entryMeta {
    clear: left;
    margin: 40px 0 0px 0; 
    padding: 5px 0 0 0;
    border-top: 1px #e0e0e0 solid;
    .tags { 
        margin: 0 0 5px 0; } }
  
#content img {
  max-width: 100%; }


/* ================================================================================= STRUCTURE == */

#pagenav {
  text-align: center;
  background: #F5F5F5;
  margin: 10px;
  padding: 10px;
  a {
    text-decoration: none;
    &:hover { }
    &.current-page { 
        font: { 
            weight: bold; } } } }


/* ================================================================================= SECONDARY CONTENT == */

ul {
  &#nav {
    list-style-type: none;
    li {
      margin-bottom: 7px; } }
  &#asides {
    list-style-type: none;
    margin-left: 5px; }
  &#moreposts {
    list-style-type: none;
    margin-left: 5px;
    li {
      padding-bottom: 5px; } }
  &#recentcomments {
    list-style-type: none;
    margin-left: 5px;
    li {
      margin-bottom: 8px;
      list-style-type: none; } } }

.block-type-twitter ul li {
  list-style-type: none;
  img {
    float: left;
    padding-right: 6px;
    margin: 3px; } }


/* ================================================================================= COMMENTS == */

.commentCount {
  float: right; }
  
#comments {
  padding-top: 20px; }

#pings {
  margin: 15px 0 25px 0; }

.ping-meta {
  background: #ddd;
  border: 1px solid #ccc;
  padding: 4px;
  margin-bottom: 15px; }

ul {
  &#pings-list, &#commentlist {
    list-style-type: none; } }

.comment {
  margin-bottom: 15px; }

.comment-content {
  border: 1px solid #ccc;
  padding: 4px;
  background: #F5F5F5; }

.comment-unapproved .comment-content {
  background: #ffc; }

.comment-meta {
  background: #ddd;
  border: 1px solid #ccc;
  padding: 4px; }
  

/* ================================================================================= FORMS == */

form { }

legend { }

label { }

input { }

textarea { width: 100%; }

input,
textarea { 
    padding: 5px 8px;
    margin: 5px 0;
    border: 2px #e0e0e0 solid; }

#submit { }

label .required { }


